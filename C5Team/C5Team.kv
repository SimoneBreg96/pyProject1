<C5Team>:
    name: "c5Team"
    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            orientation: 'horizontal'
            Button:
                id: clockBtn
                text: "Start"
                on_release:
                    root.start(10,1)
            Button:
                id: StopBtn
                text: "Stop"
                on_release:
                    root.stop()
        Label:
            id: clockLabel